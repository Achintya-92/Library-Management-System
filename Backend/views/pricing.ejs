<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Pricing & Payment</title>
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
  <style>
    
  </style>
</head>
<body class="bg-light">

  <!-- Header -->
  <header class="bg-primary text-white text-center py-4 shadow">
    <h1 class="fw-bold">Subscription Plans</h1>
    <p class="mb-0">Choose the best plan and pay securely</p>
  </header>

  <!-- Pricing Section -->
  <div class="container my-5">
    <div class="row text-center">
      <!-- One Month -->
      <div class="col-md-4 mb-4">
        <div class="card shadow h-100">
          <div class="card-body">
            <h4 class="card-title fw-bold">1 Month</h4>
            <p class="card-text fs-4 text-success">₹ 500</p>
          </div>
        </div>
      </div>
      <!-- Six Months -->
      <div class="col-md-4 mb-4">
        <div class="card shadow h-100 border-0">
          <div class="card-body">
            <h4 class="card-title fw-bold">6 Months</h4>
            <p class="card-text fs-4 text-success">₹ 2800</p>
          </div>
        </div>
      </div>
      <!-- One Year -->
      <div class="col-md-4 mb-4">
        <div class="card shadow h-100">
          <div class="card-body">
            <h4 class="card-title fw-bold">1 Year</h4>
            <p class="card-text fs-4 text-success">₹ 5000</p>
          </div>
        </div>
      </div>
    </div>
  </div>

<!-- Payment Options -->
<div class="container my-5">
  <div class="card shadow">
    <div class="card-body text-center">
      <h3 class="fw-bold">Payment Options</h3>
      <p class="text-muted">Pay with any online money transfer platform</p>
      
      <!-- QR Code -->
      <div class="mb-4">
        <h5>Scan QR:</h5>
        <img src="image.png" alt="QR Code" class="img-fluid rounded shadow" style="max-width:250px;">
      </div>

      <!-- UPI Options -->
      <h5 class="mb-3">UPI Payment Methods:</h5>
      <div class="row g-3 justify-content-center">
        
        <!-- Google Pay -->
        <div class="col-md-3 col-6">
          <!-- <a href="upi://pay?pa=example@upi&pn=MyBusiness&am=500&cu=INR"> -->
            <div class="card h-100 shadow-sm border-0">
              <div class="card-body text-center">
                <img src="data:image/jpeg;base64," alt="Google Pay" class="img-fluid mb-2" style="max-height:50px;">
                <p class="fw-semibold mb-0">Google Pay</p>
                <!-- <small class="text-muted">example@upi</small> -->
              </div>
            </div>
          <!-- </a> -->
        </div>

        <!-- PhonePe -->
        <div class="col-md-3 col-6">
          <!-- <a href="upi://pay?pa=example@upi&pn=MyBusiness&am=500&cu=INR"> -->
            <div class="card h-100 shadow-sm border-0">
              <div class="card-body text-center">
                <img src="data:image/png;base64," alt="PhonePe" class="img-fluid mb-2" style="max-height:50px;">
                <p class="fw-semibold mb-0">PhonePe</p>
                <!-- <small class="text-muted">example@upi</small> -->
              </div>
            </div>
          <!-- </a> -->
        </div>

        <!-- Paytm -->
        <div class="col-md-3 col-6">
          <!-- <a href="upi://pay?pa=example@upi&pn=MyBusiness&am=500&cu=INR"> -->
            <div class="card h-100 shadow-sm border-0">
              <div class="card-body text-center">
                <img src="https://pwebassets.paytm.com/commonwebassets/commonweb/images/instore-payments/instore-grid-5.png" alt="Paytm" class="img-fluid mb-2" style="max-height:50px;">
                <p class="fw-semibold mb-0">Paytm</p>
                <!-- <small class="text-muted">example@upi</small> -->
              </div>
            </div>
          <!-- </a> -->
        </div>

        <!-- BHIM -->
        <div class="col-md-3 col-6">
            <div class="card h-100 shadow-sm border-0">
              <div class="card-body text-center">
                <img src="data:image/jpeg;base64," alt="BHIM UPI" class="img-fluid mb-2" style="max-height:50px;">
                <p class="fw-semibold mb-0">BHIM UPI</p>
                <!-- <small class="text-muted">example@upi</small> -->
              </div>
            </div>
        </div>
      </div> <!-- end row -->
    </div>
  </div>
</div>



  <!-- Footer -->
  <footer class="bg-dark text-white text-center py-3">
    <p class="mb-0">© 2025 My Payment Page | Secure Payments</p>
  </footer>

  <!-- Bootstrap JS -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
